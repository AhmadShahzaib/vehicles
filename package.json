{
  "name": "logEld-TenantBackendMicroservices-Vehicles-Future",
  "version": "1.0.0",
  "description": "Vehicle microservice to manage vehicles related operations i.e creating, editing, updating, deleting, listings, and searchings.",
  "license": "",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon --config nodemon.json",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js",
    "build": "nest build --path ./tsconfig.json"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.4",
    "@nestjs/core": "^8.4.4",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/cli": "^10.3.0",
    "@nestjs/microservices": "^8.4.4",
    "@nestjs/mongoose": "^9.0.3",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/swagger": "^5.2.1",
    "@shafiqrathore/logeld-tenantbackend-common-future": "^1.0.65",
    "@types/babel__generator": "^7.6.4",
    "@types/multer": "^1.4.7",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "country-state-city": "^3.0.1",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.2",
    "mongoose": "^6.2.10",
    "nestjs-real-ip": "^2.1.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "rxjs": "^7.5.5",
    "swa": "^0.1.1",
    "swagger-ui-express": "^4.3.0",
    "typescript": "^4.9.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@nestjs/testing": "^8.4.4",
    "@types/babel__core": "^7.1.19",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.181",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.11.9",
    "@types/passport-jwt": "^3.0.6",
    "@types/request-ip": "^0.0.37",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.57.0",
    "globals": "^15.4.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript-eslint": "^7.13.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
